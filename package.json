{
  "name": "react-components",
  "license": "",
  "version": "0.0.1",
  "private": true,
  "description": "React Component Library",
  "author": "Charles Coqueret",
  "scripts": {
    "build": "rollup -c",
    "build-storybook": "build-storybook -o public",
    "clean": "rm -rf npm-*.log package-lock.json node_modules coverage dist && npm cache verify",
    "dev": "yarn storybook",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,js,jsx}\"",
    "format:check": "prettier --list-different \"src/**/*.{ts,tsx,json,js,jsx}\"",
    "lint": "eslint . --ext .js,.ts,.jsx,.tsx,.json --color ./",
    "lint:fix": "eslint . --ext .js,.ts,.jsx,.tsx,.json --fix --color ./",
    "nuke": "npm run clean && npm install",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "test": "jest",
    "test:coverage": "jest --maxWorkers=2 --colors --coverage",
    "test:watch": "jest --watch"
  },
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.3.0",
    "@fortawesome/pro-light-svg-icons": "^6.0.0",
    "@fortawesome/pro-regular-svg-icons": "^6.0.0",
    "@fortawesome/pro-solid-svg-icons": "^6.0.0",
    "@fortawesome/react-fontawesome": "^0.1.17",
    "@szhsin/react-menu": "^2.3.4",
    "classnames": "^2.2.6",
    "color-alpha": "^1.1.3",
    "color-rgba": "^2.4.0",
    "date-fns": "^2.28.0",
    "numeral": "^2.0.6",
    "react-datepicker": "^4.7.0",
    "react-number-format": "^4.9.1",
    "react-popper-tooltip": "^4.3.1",
    "react-select": "^5.2.2"
  },
  "peerDependencies": {
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.7",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-essentials": "^6.1.2",
    "@storybook/addon-links": "^6.1.2",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.1.2",
    "@testing-library/react": "^11.2.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/color-rgba": "^2.1.0",
    "@types/enzyme": "^3.10.11",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^26.0.15",
    "@types/node": "^12",
    "@types/numeral": "^2.0.2",
    "@types/react": "^16.9.10",
    "@types/react-datepicker": "^4.3.4",
    "@types/react-dom": "^16.9.10",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "babel-loader": "^8.2.1",
    "classnames": "^2.2.6",
    "css-loader": "^5.0.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.6",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-test-renderer": "^16.14.0",
    "rollup": "^2.69.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-typescript2": "^0.31.2",
    "sass": "^1.49.8",
    "sass-loader": "^10.1.0",
    "style-loader": "^2.0.0",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "typescript": "^3.8.3",
    "webpack": "^4.42.1"
  },
  "engines": {
    "node": ">=12.0.0"
  }
}
